{
    "cSpell.language": "en,pt-BR",
    "cSpell.words": ["Destal", "mplx", "Selec"],
    "licenser.author": "Rafael Farias",
    "licenser.license": "MIT",
    "licenser.projectName": "PIDestal",
    "licenser.customHeader": "@FILENAME@ - @PROJECT@\n\nCopyright @YEAR@ @AUTHOR@\n\nThis is my license...",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "prettier.tabWidth": 4,
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google,  ColumnLimit: 0, IndentWidth: 4, TabWidth: 4}",
    "files.associations": {
        "*.pl": "prolog",
        "array": "cpp",
        "deque": "cpp",
        "string": "cpp",
        "unordered_map": "cpp",
        "unordered_set": "cpp",
        "vector": "cpp",
        "string_view": "cpp",
        "initializer_list": "cpp",
        "regex": "cpp",
        "*.tcc": "cpp",
        "optional": "cpp",
        "system_error": "cpp",
        "future": "cpp",
        "memory": "cpp",
        "functional": "cpp",
        "istream": "cpp"
    },
    "arduino.path": "C:\\Program Files\\Arduino IDE",
    "arduino.additionalUrls": "",
    "arduino.logLevel": "info",
    "arduino.enableUSBDetection": true,
    "C_Cpp.intelliSenseEngine": "Tag Parser"
}
